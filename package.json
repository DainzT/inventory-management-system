{
    "name": "vicmar-fishing-boat-and-marketing",
    "private": true,
    "version": "0.0.0",
    "scripts": {
      "dev:frontend": "cd frontend && npm run dev",
      "dev:backend": "cd backend && npm run dev",
      "dev": "concurrently -k \"npm run dev:frontend\" \"npm run dev:backend\"",
      "start:frontend": "cd frontend && npm run build && npm run start",
      "start:backend": "cd backend && npm run build && npm run start",
      "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
      "test:jest": "cd frontend && npm run test",
      "test:integration": "cd backend && npm run test",
      "test:e2e": "cd frontend && npm run test:e2e",
      "lint": "eslint ."
    },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "concurrently": "^9.1.2"
  }
}
