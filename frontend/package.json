{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd ../backend && npm run dev",
    "start:test:backend": "cd ../backend && npm run build:test && npm run start:test",
    "start:test:frontend": "dotenv -e .env.test -- npm run build && vite preview --port 4173",
    "build": "tsc -b && vite build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:e2e": "dotenv -e .env.test -- playwright test",
    "test:e2e:ui": "dotenv -e .env.test -- playwright test --ui",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite preview --host",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
